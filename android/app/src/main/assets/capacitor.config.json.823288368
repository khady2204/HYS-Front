{
	"appId": "com.helpingyourself.hysfront",
	"appName": "HYS-Front",
	"webDir": "www",
	"server": {
		"cleartext": true,
		"androidScheme": "http"
	},
	"plugins": {
		"Keyboard": {
			"resize": "body",
			"style": "light",
			"scrollAssist": true,
			"resizeOnFullScreen": true
		},
		"Camera": {
			"android": {
				"enableCameraPermission": true,
				"enableGalleryPermission": true
			}
		}
	}
}
